<main class="product-management">
    <header class="user-info">
        <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/45881b31095ea0d6211c0f58a4f1d6517c033b88cf60a2c244319e416b16228a?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="User avatar" class="user-avatar">
        <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/0dfd0ea1ea90f0839626abdad5fadce1d87e33dadcf5fc0b635aa884bfddfd78?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Notification icon" class="notification-icon">
        <div class="user-details">
            <div class="user-name-wrapper">
                <h2 class="user-name">abc</h2>
                <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/d3fa7866172b7d04eb7cb03a8661a373ef14397ae779752e06235e77e53b6843?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="User status indicator" class="status-indicator">
            </div>
            <p class="user-email">abcgmail.com</p>
        </div>
    </header>

    <section class="management-header">
        <h1 class="management-title">PRODUCT MANAGEMENT</h1>
        <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/af0e95019fbcd765549092b28f0fbf6a9193ed4dc45a1fa25a75cd00d41c5def?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Product management illustration" class="management-illustration">
    </section>

    <section class="product-list">
        <div class="product-list-header">
            <h2 class="product-count">PRODUCTS <span>(100)</span></h2>
            <div class="filter-options">
                <button class="filter-button">
                    All
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/1b543a329805a33313758c0f0b3d78bad5839bbddef5b88e4e51a2b648a6268b?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Expand all filter" class="expand-icon">
                </button>
                <button class="filter-button">
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/ec8d0d8a78035897d72b97b5c3c0b2040dac2441c5388d0d454600c9b2daf362?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Calendar icon" class="calendar-icon">
                    Today
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/1b543a329805a33313758c0f0b3d78bad5839bbddef5b88e4e51a2b648a6268b?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Expand today filter" class="expand-icon">
                </button>
                <button class="filter-button">
                    Columns
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/1b543a329805a33313758c0f0b3d78bad5839bbddef5b88e4e51a2b648a6268b?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Expand columns filter" class="expand-icon">
                </button>
            </div>
        </div>

        <table class="product-table">
            <thead>
                <tr>
                    <th>
                        <input type="checkbox" id="select-all" class="select-checkbox">
                        <label for="select-all" class="visually-hidden">Select all products</label>
                    </th>
                    <th>Product Info</th>
                    <th>Category ID</th>
                    <th>Size</th>
                    <th>Quantity</th>
                    <th>Price</th>
                    <th>Status</th>
                    <th>Detail</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <input type="checkbox" id="product1" class="select-checkbox">
                        <label for="product1" class="visually-hidden">Select product 1</label>
                    </td>
                    <td>
                        <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/8cc7825881f09db67770217d3a5d4d29da9a20c75c19b2282c826e1c3fa086c4?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Product 1 image" class="product-image">
                        <div class="product-info">
                            <span class="product-id">P-1</span>
                            <span class="product-name">Bún Đậu</span>
                        </div>
                    </td>
                    <td>C-1</td>
                    <td>ABC</td>
                    <td><!-- Quantity --></td>
                    <td><!-- Price --></td>
                    <td>
                        <span class="status-indicator available"></span>
                        Available
                    </td>
                    <td>
                        <button class="view-button">
                            <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/0f380020b2551aece22984d261fa3c181f421dca5da8af54a2283f3ed7fbaea2?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="View details" class="view-icon">
                            View
                        </button>
                    </td>
                </tr>
                <!-- Repeat similar structure for other products -->
            </tbody>
        </table>

        <footer class="product-list-footer">
            <p class="selection-info">0 of 13 raw(s) selected</p>
            <nav class="pagination" aria-label="Product list navigation">
                <button class="pagination-button" aria-label="First page">
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/5ee9be6e5db5801a2482b3c98bb293c259aa6d0b33cc463f68f86993c801ed7a?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="" class="pagination-icon">
                </button>
                <button class="pagination-button" aria-label="Previous page">
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/5ab012c4f88b07e8f612307998d88d7f5f231aef4b942953b6c74e2962f46df4?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="" class="pagination-icon">
                </button>
                <ul class="page-numbers">
                    <li><a href="#" aria-current="page">1</a></li>
                    <li><a href="#">2</a></li>
                    <li><a href="#">3</a></li>
                    <li><span>...</span></li>
                </ul>
                <button class="pagination-button" aria-label="Next page">
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/606e83e6ef644234eb3b41e6273e4711e13fecc706dfd58ea4ddcfa61d5a2c65?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="" class="pagination-icon">
                </button>
                <button class="pagination-button" aria-label="Last page">
                    <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/e69e46eea12f3687bc746aaec319b82ea34ce3f95fc02e7e1a5aaddb8a63cb50?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="" class="pagination-icon">
                </button>
            </nav>
            <img src="https://cdn.builder.io/api/v1/image/assets/fd43dbd6411c4856a145ca3e16eab82b/d6fc944d6d21cffaac72e7d6f57181bf77c6ec6655d0bd7ae7c3ca05967a602c?apiKey=fd43dbd6411c4856a145ca3e16eab82b&" alt="Decorative element" class="decorative-image">
        </footer>
    </section>
</main>
